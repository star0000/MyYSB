<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="410px"
    android:layout_marginTop="20px"
    android:layout_marginLeft="30px"
    android:layout_marginRight="30px"
    android:orientation="vertical"
    android:background="@drawable/self_info_back_shape"
    android:descendantFocusability="blocksDescendants">

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20px"
        android:gravity="center_vertical"
        android:orientation="horizontal" >
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            >
            <TextView
                android:id="@+id/baseline"
                android:text="000"
                android:textSize="58px"
                android:textColor="#ff89a8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="10px"
                >
                <TextView
                    android:textColor="#717171"
                    android:text="bpm"
                    android:textSize="12px"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:textColor="#717171"
                    android:text="基线"
                    android:textSize="24px"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </LinearLayout>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginTop="30px"
            android:layout_marginBottom="30px"
            android:background="@drawable/taixinjilu_line" />
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            >

            <TextView
                android:id="@+id/fm"
                android:text="000"
                android:textSize="58px"
                android:textColor="#ff89a8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginLeft="10px"
                >
                <TextView
                    android:textColor="#717171"
                    android:text="次"
                    android:textSize="12px"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:textColor="#717171"
                    android:text="胎动"
                    android:textSize="24px"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

        </LinearLayout>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginTop="30px"
            android:layout_marginBottom="30px"
            android:background="@drawable/taixinjilu_line"
            />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <TextView
                android:textColor="#ff89a8"
                android:id="@+id/type"
                android:text="类型"
                android:textSize="24px"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                />
            <TextView
                android:textColor="#717171"
                android:text="变异类型"
                android:textSize="24px"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginTop="30px"
            android:layout_marginBottom="30px"
            android:background="@drawable/taixinjilu_line"
            />
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            >
            <TextView
                android:textColor="#717171"
                android:id="@+id/acc"
                android:text="acc"
                android:textSize="24px"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                />

            <TextView
                android:textColor="#717171"
                android:id="@+id/result"
                android:textSize="24px"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                />
        </LinearLayout>
        <!--android:visibility="gone" 隐藏显示  -->
        <TextView
            android:textColor="@color/blue_b_color"
            android:id="@+id/guid"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/string_guid"
            android:textAppearance="?android:attr/textAppearanceLarge" />

    </LinearLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="25px"
        android:layout_marginBottom="20px"
        >
        <LinearLayout
            android:id="@+id/ll_taixin_right"
            android:layout_width="170px"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:gravity="center_horizontal"
            android:orientation="vertical">
            <ImageView
                android:id="@+id/imageButton1"
                android:layout_width="80px"
                android:layout_height="80px"
                android:layout_marginTop="50px"
                android:focusable="false"
                android:src="@drawable/list_a" />
            <!--时间-->
            <TextView
                android:layout_marginTop="20px"
                android:id="@+id/time"
                android:text="time"
                android:textSize="24px"
                android:textColor="#99717171"
                android:layout_gravity="center_horizontal"
                android:gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                />
        </LinearLayout>
        <ImageView
            android:id="@+id/img"
            android:layout_width="match_parent"
            android:layout_toLeftOf="@+id/ll_taixin_right"
            android:layout_height="250px"
            android:layout_marginLeft="20px"
            android:contentDescription="@string/string_img"
            android:scaleType="fitXY"
            android:background="@drawable/contacts_selected" />

    </RelativeLayout>


</LinearLayout>